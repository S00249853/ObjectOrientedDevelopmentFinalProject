<Window x:Class="OOD_Project.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OOD_Project"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="153*"/>
            <RowDefinition Height="64*"/>
        </Grid.RowDefinitions>
        <Button x:Name="createBtn" Content="Create Account" HorizontalAlignment="Left" Margin="179,210,0,0" VerticalAlignment="Top" Height="35" Width="129" Click="createBtn_Click"/>
        <Button x:Name="logBtn" Content="Log In" HorizontalAlignment="Left" Margin="499,210,0,0" VerticalAlignment="Top" Height="35" Width="129" Click="logBtn_Click"/>

    </Grid>
</Window>
